  "version": "1.0.0",
  "description": "Node.js SDK (and CLI) for StdLib",
  "main": "index.js",
  "bin": {
    "stdlibjs": "cli.js"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
