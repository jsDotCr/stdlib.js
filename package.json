  "dependencies": {
    "chalk": "2.1.0",
    "debug": "3.0.0",
    "dotenv": "4.0.0",
    "glob": "7.1.2",
    "got": "7.1.0",
    "joi": "10.6.0",
    "tar-fs": "1.15.3",
    "yargs": "8.0.2"
  },
  "devDependencies": {
    "chai": "4.1.1",
    "chai-as-promised": "7.1.1",
    "codecov": "2.3.0",
    "mocha": "3.5.0",
    "npm-check-updates": "2.12.1",
